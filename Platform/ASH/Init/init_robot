# Init script for ASH robot

sudo modprobe -r pcan
sudo modprobe pcan
sudo ip link set dev can0 up
sudo ip link set dev can1 up
sudo ip link set dev can2 up
sudo ip link set dev can3 up
sudo su -c "echo 'i 0x0014 e' > /dev/pcan0"
sudo su -c "echo 'i 0x0014 e' > /dev/pcan1"
sudo su -c "echo 'i 0x001c e' > /dev/pcan2"
sudo su -c "echo 'i 0x001c e' > /dev/pcan3"
cat /proc/pcan
